    <header class="header">
      <div class="header__bar row">
        <h1 class="grid-6"><a href="/">Bookshelf</a></h1>
        <div class="user_nav grid-6">
          <a class="post" href="/books/new">投稿する</a>
        </div>
      </div>
    </header>

<div class="shelf">
  <div class="contents-row">
    <% @books.each do |book| %>
      <div class='item'>
        <ul class="name">
          <% if book.id != 0%>
            <div class="content_post" >
              <li class="list">
                <%= book.title %>
              </li>
            </div>
          <% end %>
        </ul>
      </div>
    <% end %>
  </div>
</div>
